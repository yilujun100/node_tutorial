{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": "off",
        "no-tabs": "off",
        "linebreak-style": "off",
        "arrow-parens": ["error", "as-needed"],
        "func-names": "off",
        "comma-dangle": "off"
    }
}
